<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dictionary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">

        <activity android:name=".phrases" android:label="@string/phrases" android:parentActivityName=".MainActivity"/>
        <activity android:name=".family"  android:label="@string/family"  android:parentActivityName=".MainActivity"/>
        <activity android:name=".number"  android:label="@string/number"  android:parentActivityName=".MainActivity"/>
        <activity android:name=".Color"   android:label="@string/color"   android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>